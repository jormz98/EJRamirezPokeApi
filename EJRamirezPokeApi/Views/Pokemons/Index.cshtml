@{
    ViewData["Title"] = "Pokemons";
}

    <h3>Lista de pokemons <img class="logoPokemon" src="data:image/png;base64," alt="pokemon"></h3>

<div class="containerHeader">
    <select id="typeRegisters">
       
    </select>
    <select id="numberRegisters" data-urlactual="">
        <option value="10">10</option>
        <option value="20">20</option>
        <option value="50">50</option>
        <option value="100">100</option>
    </select>
    <div class="containerInput" id="containerInput">
        <label class="labelHolder">Busca un pokemon</label>
        <input type="text" class="inp" id="inputSearch">
        <i class="fa-brands fa-searchengin"></i>
    <button id="btnSearch">Buscar</button> 
    </div>
</div>





    <h4 id="countRegisters"></h4>

<div class="containerCards" id="containerCards">
</div>


<div class="containerFooter" id="containerFooter">
    <h5 id="InfoRegistros"></h5>
    <br>
    <div class="containerPaginador">
        <button class="btnPage" id="btnPrevious" data-urlprevious>Previous</button>
        <div class="containerPaginas" id="containerPaginador">
        
        </div>
        <button class="btnPage" id="btnNext" data-urlnext>Next</button>
    </div>
</div>




















<template id="tmptCard">

    <div class="card">
        <img class="fotoPoke" src="" alt="">
        <br>
        <h4 class="namePoke"> Charmander </h4>
        <button data-urlid="" class="btnPlus"> Saber mas </button>
    </div>

</template>







<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable modal-lg">
        <div class="modal-content">
            <div class="modal-header" id="modalHeader">
                <h5 class="modal-title" id="exampleModalLabel"></h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">

                <div class="containerBodyModal">
                    <div>
                        <img class="imgPokemon" id="imgPokemon" src="" alt="">
                        <p id="pTipos"></p>
                    </div>
                   
                    <div class="containerInfoPokemon" id="containerInfoPokemon">
                       
                    </div>

                </div>

                
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>  
            </div>
        </div>
    </div>
</div>









@section Scripts{

    <script src="~/js/appPokeApi.js"></script>

}


